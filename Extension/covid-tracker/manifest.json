{
  "name": "Covid-19",
 "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "covid2.png"
  },
  "icons": {
    "128": "covid2.png"
  },
   "content_security_policy": "script-src 'self' https://unpkg.com ; object-src 'self'",
  "permissions": ["activeTab"]
}
